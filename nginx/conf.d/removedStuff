    location / {
        rewrite ^/(\.well-known/.*)$ /$1 break;
        rewrite ^/$ /webroot/ break;
        rewrite ^(.*)$ /webroot/$1 break;
        try_files $uri $uri/ /index.php?$query_string;
    }

    location /webroot {
          if (!-e $request_filename){
            rewrite ^(.*)$ /index.php break;
          }
    }
